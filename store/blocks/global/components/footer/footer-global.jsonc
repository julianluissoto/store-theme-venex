{
  "footer#desktop": {
    "blocks": [
      "footer-layout.desktop#desktop__footer--mainContainer",
      "footer-layout.mobile#mobile__footer--mainContainer"
    ]
  },
  "footer-layout.desktop#desktop__footer--mainContainer": {
    "title": "desktop___footer--Main-container",
    "props": {
      "blockClass": "desktop__footer--mainContainer"
    },
    "children": [
      "flex-layout.row#footer__desktop--linksContainer",
      "flex-layout.row#footer__desktop--titles",
      "flex-layout.row#footer__desktop--info"
    ]
  },

  "flex-layout.row#footer__desktop--titles": {
    "title": "footer titles",
    "props": {
      "blockClass": "footer__desktop--titles"
    },
    "children": [
      "rich-text#customerService__title-1",
      "rich-text#customerService__title-2",
      "rich-text#customerService__title-3"
    ]
  },
  "flex-layout.row#footer__desktop--info": {
    "title": "footer dekstop info",
    "props": {
      "blockClass": "footer__desktop--info"
