{
  "flex-layout.row#desktop__buyingOptions--container": {
    "title": "buying option main container",
    "props": {
      "blockClass": "desktop__buyingOptions--Maincontainer"
    },
    "children": [
      "flex-layout.col#desktop__buyingOptions--container-col"
    ]
  },
  "flex-layout.col#desktop__buyingOptions--container-col": {
    "title": "buyng options columns",
