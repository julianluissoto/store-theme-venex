    "title": "image credit card option",
    "props": {
      "src": "assets/img/modal-images/payment-options.png",
      "blockClass": "shiping__image"
    }
  },
  "image#Hipping_image": {
    "title": "image credit card",
    "props": {
      "src": "assets/img/banner-shiping-img/envio_express_footer.png",
      "blockClass": "shiping__image"
    }
  },

  "image#creditCard__image": {
    "title": "image credit card",
    "props": {
      "src": "assets/img/banner-shiping-img/card.png",
      "blockClass": "creditCard__image"
    }
  },
  "image#creditCard__image-2": {
    "title": "image credit card",
    "props": {
      "src": "assets/img/banner-shiping-img/grey-truck.png",
      "blockClass": "creditCard__image"
    }
  },
  "rich-text#creditOptions__text-1": {
    "title": "image credit text",
    "props": {
      "blockClass": "creditOptions__text-1",
      "text": " UP TO 12 PAYMENTS "
    }
  },
  "rich-text#creditOptions__text-2": {
    "title": "image credit text",
    "props": {
      "blockClass": "creditOptions__text-2",
      "text": " with mayor credit cards",
      "font": "heading-3"
    }
  }

