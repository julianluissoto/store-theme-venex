    "title": "buying option main container",
    "props": {
      "blockClass": "desktop__buyingOptions--container"
    },
    "children": [
      "flex-layout.col#creditCard__container",
      "flex-layout.col#receibeProduct__container",
      "flex-layout.col#creditCard__container-3"
    ]
  },
  "flex-layout.col#creditCard__container": {
    "title": "credit card col container",
    "props": {
      "blockClass": "creditCard__container",
      "with": "33%"
